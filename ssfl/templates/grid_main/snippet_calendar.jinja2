<div class="title text-wrap">Upcoming Events</div>
<div>
    {% for event in events %}
        <div class="mt-3">
            <p class="h5 font-weight-bold">{{ event.name }}</p>
            <p class="my-0">{{ event.date }} {{ event.time }}</p>
            <p class="my-0">{{ event.venue }}</p>
        </div>
    {% endfor %}
</div>
